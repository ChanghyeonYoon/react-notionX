{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "composite": true,
    "rootDir": "src",
    "outDir": "build",
    "tsBuildInfoFile": "build/.tsbuildinfo",
    "emitDeclarationOnly": true,
    "lib": ["DOM", "ESNext"],
    "noImplicitReturns": false,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "include": ["src"],
  "references": [{ "path": "../notion-types" }, { "path": "../notion-utils" }]
}
